decl
    number a, b;
    string str;
enddecl

begin 

    str = "HiWorld";
    write(str);

    a = 1;
    read(b);
    write((a+b)*b);

    while(a < b) do
        write(a);

        if(a < 5) then
            write(0);
        else
            write(1);
        endif

        if (a == b - 1) then
            write(100);
        endif

        a = a + 1;
    endwhile 
end